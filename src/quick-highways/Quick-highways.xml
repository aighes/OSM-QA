<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0"
	shortdescription="Quick-highways"
	description="Presets for quickly adding popular highway=* and surface=* tags"
	version="0.1">
	<group name="Quick-highways" icon="motorway.png">
		<group name="highway=*" icon="unclassified.png">
			<item name="motorway" de.name="Autobahn" icon="motorway.png" type="way">
				<key key="highway" value="motorway" />
			</item>
			<item name="trunk" de.name="Autobahnähnliche Straße" icon="trunk.png" type="way,closedway">
				<key key="highway" value="trunk" />
			</item>
			<item name="primary" de.name="Bundesstraße" icon="primary.png" type="way,closedway">
				<key key="highway" value="primary" />
			</item>
			<item name="secondary" de.name="Landesstraße" icon="secondary.png" type="way,closedway">
				<key key="highway" value="secondary" />
			</item>
			<item name="tertiary" de.name="Kreisstraße" icon="tertiary.png" type="way,closedway">
			        <key key="highway" value="tertiary" />
			</item>
			<group name="highway link" icon="motorway_link.png">
				<item name="motorway_link" de.name="Autobahnauffahrt" type="way,closedway" icon="motorway_link.png">
				    	<key key="highway" value="motorway_link" />
				</item>
				<item name="trunk_link" de.name="Schnellstraßenauffahrt" type="way" icon="trunk_link.png" >
					<key key="highway" value="trunk_link" />
				</item>
				<item name="primary_link" de.name="Bundesstraßenauffahrt" type="way" icon="primary_link.png">
				    	<key key="highway" value="primary_link" />
				</item>
				<item name="secondary_link" de.name="Landesstraßenauffahrt" type="way" icon="secondary_link.png">
				    	<key key="highway" value="secondary_link" />
				</item>
				<item name="tertiary_link" de.name="Kreisstraßenauffahrt" type="way" icon="tertiary_link.png">
		  		  	<key key="highway" value="tertiary_link" />
				</item>
			</group>
			<item name="unclassified" de.name="Nebenstraße" icon="unclassified.png" type="way,closedway">
				<key key="highway" value="unclassified" />
			</item>
			<item name="residential" de.name="Wohnstraße" icon="residential.png" type="way,closedway">
				<key key="highway" value="residential" />
			</item>
			<item name="service" de.name="Erschließungsweg" icon="service.png" type="way,closedway">
				<key key="highway" value="service" />
			</item>
			<item name="track" de.name="Wirtschaftsweg" icon="track.png" type="way,closedway">
				<key key="highway" value="track" />
			</item>
			<item name="living street" de.name="Verkehrsberuhigter Bereich" icon="living_street.png" type="way,closedway">
				<key key="highway" value="living_street" />
			</item>
			<group name="footway" de.name="Fußweg" icon="footway.png">
				<item name="footway" de.name="Fußweg" icon="footway.png" type="way,closedway">
					<key key="highway" value="footway" />
				</item>
				<item name="Sidewalk" icon="sidewalk.png" type="way,closedway" >
					<key key="highway" value="footway" />
					<key key="footway" value="sidewalk"/>
				</item>
				<item name="Traffic island" icon="footway_traffic_island.png" type="way">
					<key key="highway" value="footway" />
					<key key="footway" value="traffic_island"/>
				</item>
				<item name="Crossing" icon="footway_crossing.png" type="way,closedway">
					<key key="highway" value="footway" />
					<key key="footway" value="crossing"/>
				</item>
				<item name="Link" icon="footway_link.png" type="way">
					<key key="highway" value="footway" />
					<key key="footway" value="link"/>
				</item>
			</group>
			<group name="cycleway" de.name="Radweg" icon="cycleway.png">
				<item name="cycleway" de.name="Radweg" icon="cycleway.png" type="way,closedway">
					<key key="highway" value="cycleway" />
				</item>
				<item name="Sidepath" icon="sidepath.png" type="way,closedway">
					<key key="highway" value="cycleway" />
					<key key="cycleway" value="sidepath"/>
				</item>
				<item name="Traffic island" icon="cycleway_traffic_island.png" type="way">
					<key key="highway" value="cycleway" />
					<key key="cycleway" value="traffic_island"/>
				</item>
				<item name="Crossing" icon="cycleway_crossing.png" type="way">
					<key key="highway" value="cycleway" />
					<key key="cycleway" value="crossing"/>
				</item>
				<item name="Link" icon="cycleway_link.png" type="way">
					<key key="highway" value="cycleway" />
					<key key="cycleway" value="link"/>
				</item>
			</group>
			<item name="path" de.name="Pfad" icon="path.png" type="way,closedway">
				<key key="highway" value="path" />
			</item>
			<item name="steps" de.name="Treppe" icon="steps.png" type="way,closedway">
				<key key="highway" value="steps" />
			</item>
		</group>
		<group name="surface=*" icon="asphalt.png">
			<item name="asphalt" de.name="Asphalt" icon="asphalt.png" type="way,closedway">
				<key key="surface" value="asphalt" />
			</item>
			<item name="concrete" de.name="Beton" icon="concrete.png" type="way,closedway">
				<key key="surface" value="concrete" />
			</item>
			<item name="paving_stones" de.name="Pflastersteine" icon="paving_stones.png" type="way,closedway">
				<key key="surface" value="paving_stones" />
			</item>
			<item name="gravel" de.name="Schotter" icon="gravel.png" type="way,closedway">
				<key key="surface" value="gravel" />
			</item>
			<item name="wood" de.name="Holzbretter" icon="wood.png" type="way,closedway">
				<key key="surface" value="wood" />
			</item>
			<item name="ground" de.name="Bewachsene Erde" icon="dirt.png" type="way,closedway">
				<key key="surface" value="ground" />
			</item>
			<item name="sand" de.name="Sand" icon="sand.png" type="way,closedway">
				<key key="surface" value="sand" />
			</item>
			<item name="grass" de.name="Gras" icon="grass.png" type="way,closedway">
				<key key="surface" value="grass" />
			</item>
		</group>
		<group name="lanes" icon="3-lane.png">
			<item name="no markings" type="way">
				<key key="lane_markings" value="no" />
			</item>
			<item name="1 lane" icon="1-lane.png" type="way">
				<key key="lanes" value="1" />
			</item>
			<item name="2 lanes" icon="2-lane.png" type="way">
				<key key="lanes" value="2" />
			</item>
			<item name="3 lanes" icon="3-lane.png" type="way">
				<key key="lanes" value="3" />
			</item>
			<item name="4 lanes" icon="4-lane.png" type="way">
				<key key="lanes" value="4" />
			</item>
		</group>
		<group name="additional" de.name="zusätzlich" icon="bridge.png">
			<item name="bridge" de.name="Brücke" icon="bridge.png" type="way">
				<key key="bridge" value="yes" />
				<key key="layer" value="1" />
			</item>
			<item name="tunnel" de.name="Tunnel" icon="tunnel.png" type="way">
				<key key="tunnel" value="yes" />
				<key key="layer" value="-1" />
			</item>
			<item name="Culvert" icon="presets/transport/passage/tunnel_culvert.svg" type="node,way">
				<key key="tunnel" value="culvert" />
				<key key="layer" value="-1" />
			</item>
			<item name="building_passage" de.name="Gebäude-Passage" icon="building-passage.png" type="way">
				<key key="tunnel" value="building_passage" />
			</item>
			<item name="covered" de.name="überdacht" icon="" type="way">
				<key key="covered" value="yes" />
			</item>
			<item name="Oneway" de.name="Einbahnstraße" icon="oneway.svg" type="way">
				<key key="oneway" value="yes" />
			</item>
		</group>
		<group name="traffic signs" icon="presets/vehicle/traffic_signals.svg">
			<group name="traffic lights" icon="presets/vehicle/traffic_signals.svg">
				<item name="forward" icon="" type="node">
					<key key="highway" value="traffic_signals"/>
					<key key="traffic_signals" value="traffic_lights"/>
					<key key="traffic_signals:direction" value="forward"/>
				</item>
				<item name="backward" icon="" type="node">
					<key key="highway" value="traffic_signals"/>
					<key key="traffic_signals" value="traffic_lights"/>
					<key key="traffic_signals:direction" value="backward"/>
				</item>
			</group>
			<group name="all way stop" icon="presets/vehicle/restriction/stop.svg">
				<item name="forward" icon="" type="node">
					<key key="highway" value="stop"/>
					<key key="stop" value="all"/>
					<key key="direction" value="forward"/>
				</item>
				<item name="backward" icon="" type="node">
					<key key="highway" value="stop"/>
					<key key="stop" value="all"/>
					<key key="direction" value="backward"/>
				</item>
			</group>
			<group name="minor stop" icon="presets/vehicle/restriction/stop.svg">
				<item name="forward" icon="" type="node">
					<key key="highway" value="stop"/>
					<key key="stop" value="minor"/>
					<key key="direction" value="forward"/>
				</item>
				<item name="backward" icon="" type="node">
					<key key="highway" value="stop"/>
					<key key="stop" value="minor"/>
					<key key="direction" value="backward"/>
				</item>
			</group>
			<group name="yield" icon="presets/vehicle/restriction/give_way.svg">
				<item name="forward" icon="" type="node">
					<key key="highway" value="give_way"/>
					<key key="direction" value="forward"/>
				</item>
				<item name="backward" icon="" type="node">
					<key key="highway" value="give_way"/>
					<key key="direction" value="backward"/>
				</item>
			</group>
		</group>
	</group>
</presets>
